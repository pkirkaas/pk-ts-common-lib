{
  "name": "pk-ts-common-lib",
  "version": "l.0.1",
  "author": "Paul Kirkaas",
  "license": "UNLICENSED",
  "description": "TS/JS Utilities for both client & node apps",
  "engines": {
    "node": ">= 18.0.0"
  },
  "comments": {
    "mainjs": "dist/index.js",
    "main": "dist/index",
    ".": "./dist/index",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.js"
      }
    },
    "end": "end"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "type": "commonjs",
  "scripts": {
    "postinstall": "tsc",
    "clean": "rm -rf ./dist ./node_modules package-lock.json"
  },
  "dependencies": {
    "axios": "*",
    "date-fns": "latest",
    "error-stack-parser": "*",
    "file-type": "latest",
    "json-decycle": "*",
    "json5": "*",
    "lodash": "latest",
    "stack-trace": "*",
    "stacktracey": "*",
    "body-parser": "*",
    "cookie-parser": "*",
    "cors": "*",
    "ts-node": "*",
    "uuidv4": "*",
    "tsconfig-paths": "*",
    "url-status-code": "^2.0.0",
    "url-join": "latest",
    "uuid": "*"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/lodash": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "prettier": "*",
    "typescript": "*"
  }
}
