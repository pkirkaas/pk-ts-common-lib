{
  "name": "pk-ts-common-lib",
  "author": "Paul Kirkaas",
  "license": "UNLICENSED",
  "description": "TS/JS Utilities for both client & node apps",
  "engines": {
    "node": ">= 20.0.0"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./tsconfig.cjs.json": "./tsconfig.cjs.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json",
    "clean": "rm -rf ./dist ./node_modules package-lock.json && npm i",
    "monitor": "nodemon --exec \"npm run build\" --ext js,jsx,ts,tsx,sass,css --delay 5 --watch ./src",
    "reset": "npm run clean && npm run build",
    "tstarg": "echo Something and --"
  },
  "dependencies": {
    "axios": "*",
    "date-fns": "latest",
    "error-stack-parser": "*",
    "file-type": "latest",
    "json-decycle": "^3.0.0",
    "json5": "*",
    "lodash": "latest",
    "prettier": "*",
    "stack-trace": "*",
    "stacktracey": "*",
    "url-join": "latest",
    "url-status-code": "*"
  },
  "devDependencies": {
    "@types/lodash": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "nodemon": "*",
    "ts-node": "*",
    "tsconfig-paths": "*",
    "typescript": "*"
  }
}
